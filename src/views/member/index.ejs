    fetch("/api/webapi/GetUserInfo")
      .then((response) => response.json())
      .then((data) => {
        if (data.status === false) {
          unsetCookie();
          return false;
        }
        $(".infoName .name").text(`Tài khoản: ${data.data.phone_user}`);
        $(".infoName .id").text(`Số dư: ${data.data.money_user}.00 USD`);
        $(".infoName .id").attr("data-clipboard-text", "");
        $(".infoName .number").text(`Điện thoại: ${data.data.phone_user}`);
        $(".money .txt").text(` ${data.data.money_user}.00 ₫ `);
      });
  </script>

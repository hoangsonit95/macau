    <script>
        var authCookie = getCookie('auth');
        var tokenCookie = getCookie('token');

        if (authCookie && tokenCookie) {
            document.querySelector('.navbar-right_login').style.display = 'none';
        } else {
            document.querySelector('.navbar-right_user').style.display = 'none';
        }

        function getCookie(name) {
            var cookieArr = document.cookie.split(';');
            for (var i = 0; i < cookieArr.length; i++) {
                var cookiePair = cookieArr[i].trim().split('=');
                if (cookiePair[0] === name) {
                    return decodeURIComponent(cookiePair[1]);
                }
            }
            return null;
        }</script>

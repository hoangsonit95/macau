        async function addEnvent() {
            let html5 = 0;
            if (
                /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(
                    navigator.userAgent
                )
            ) {
                html5 = 1;
            }
            $('.launch-game').click(function () {
                let providercode = $(this).attr('data');
                let type = $(this).attr('type');
                $('.Loading').fadeIn(0);
                $.ajax({
                    type: 'POST',
                    url: '/api/webapi/launchgame',
                    data: {
                        providercode,
                        type,
                        html5,
                    },
                    dataType: 'json',
                    success: function (res) {
                        let { result } = res;
                        if (result.errMsg === 'SUCCESS') {
                            window.open(result.gameUrl, '_blank');
                        } else {
                            $('.van-overlay').addClass('open');
                        }
                        $('.Loading').fadeOut(0);
                    },
                });
            });
        }
